{
	"manifest_version": 3,
	"name": "Block .ZIP Domains",
	"version": "1.0",
	"description": "Block all .zip top-level domain websites",
	"permissions": ["webRequest"],
	"background": {
	  "service_worker": "background.js"
	},
	"content_security_policy": {
	  "extension_pages": "script-src 'self'; object-src 'self';"
	}
}
